<mods xmlns:biblio="http://www.stg.brown.edu/biblio/" xmlns="http://www.loc.gov/mods/v3" version="3.2">
    <titleInfo biblio:id="titleInfo">
        <title biblio:id="title"/>
        <subTitle biblio:id="subtitle"/>
    </titleInfo>
    <identifier biblio:id="DHQIdentifier" type="local"/>
    <abstract biblio:id="abstract"/>
    <name biblio:id="name" type="personal">
        <namePart biblio:id="fullName"/>
        <namePart type="given" biblio:id="givenName"/>
        <namePart type="family" biblio:id="familyName"/>
        <role>
            <roleTerm type="text" biblio:id="role"/>
        </role>
    </name>
    <name biblio:id="conferenceName" type="conference">
        <namePart biblio:id="conferenceNamePart"/>
        <role>
            <roleTerm biblio:id="conferenceRole" type="text"/>
        </role>
    </name>
    <name biblio:id="corporateName" type="corporate">
        <namePart biblio:id="corporateNamePart"/>
    </name>
    <typeOfResource biblio:id="typeOfResource"/>
   <!-- genre with @authority = 'biblio' is reserved for handling Genre type within the biblio system -->
   <!-- Please do not add this as an editable field -->
    <genre authority="marcgt" biblio:id="marcGenre"/>
    <genre biblio:id="genre"/>
    <originInfo biblio:id="originInfo">
        <place>
            <placeTerm authority="marccountry" type="code" biblio:id="placeCode"/>
            <placeTerm type="text" biblio:id="place"/>
        </place>
        <publisher biblio:id="publisher"/>
        <dateIssued biblio:id="date"/>
        <issuance biblio:id="issuance"/>
    </originInfo>
    <physicalDescription biblio:id="physicalDescription">
        <form authority="marcform" biblio:id="form"/>
        <extent biblio:id="extent"/>
    </physicalDescription>
    <subject biblio:id="subject">
        <topic biblio:id="subjectTopic"/>
    </subject>
    <relatedItem biblio:id="hostItem" type="host">
        <titleInfo>
            <title biblio:id="hostTitle"/>
            <subTitle biblio:id="hostSubtitle"/>
        </titleInfo>
        <name biblio:id="hostName" type="personal">
            <namePart type="given" biblio:id="hostGivenName"/>
            <namePart type="family" biblio:id="hostFamilyName"/>
            <role>
                <roleTerm type="text" biblio:id="hostRole"/>
            </role>
        </name>
        <originInfo>
            <place>
                <placeTerm type="text" biblio:id="hostPlace"/>
            </place>
            <issuance biblio:id="hostIssuance"/>
            <publisher biblio:id="hostPublisher"/>
            <dateIssued biblio:id="hostDate"/>
        </originInfo>
        <part>
            <detail type="volume">
                <number biblio:id="hostVolume"/>
            </detail>
            <detail type="issue">
                <number biblio:id="hostIssueNum"/>
            </detail>
            <extent unit="pages">
                <start biblio:id="hostFirstPage"/>
                <end biblio:id="hostLastPage"/>
            </extent>
            <date biblio:id="hostPartDate"/>
        </part>
        <subject biblio:id="hostSubject">
            <topic biblio:id="hostSubjectTopic"/>
        </subject>
        <location biblio:id="hostLocation">
            <url biblio:id="hostUrl"/>
        </location>
    </relatedItem>
    <location biblio:id="location">
        <url biblio:id="url"/>
    </location>
    <recordInfo>
        <recordOrigin biblio:id="recordCreatorEditor"/>
    </recordInfo>
    <note biblio:id="note"/>
    <note biblio:id="flagNote" type="flag"/>
</mods>